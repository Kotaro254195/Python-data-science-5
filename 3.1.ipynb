    "from bs4 import BeautifulSoup\n",
    "import re\n",
    "\n",
    "email_example=\"\"\"\n",
    "abc@emailgmail.com\n",
    "\"\"\"\n",
    "\n",
    "soup_email=BeautifulSoup(email_example,\"lxml\")\n",
    "email_ID_regexp=re.compile(\"\\w+@\\w+\\.\\w+\")\n",
    "email_id=soup_email.find(text=email_ID_regexp)\n",
    "print(email_id)"
